{
    "name": "doxdocgen",
    "displayName": "Doxygen Documentation Generator",
    "description": "Generate doxygen documentation from source code",
    "version": "0.0.3",
    "publisher": "cschlosser",
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:cpp"
    ],
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Doxygen Documentation Generator Settings",
            "properties": {
                "doxdocgen.generic.commentStart": {
                    "description": "Doxygen comment start indicator. Default ist /** but Qt style with /*! is also valid.",
                    "type": "string",
                    "default": "/**",
                    "enum": [
                        "/**",
                        "/*!"
                    ]
                }
            }
        }
    },
    "icon": "images/icon.png",
    "keywords": [
        "cpp", "c++", "c", "doxygen"
    ],
    "license": "SEE LICENSE IN LICENSE",
    "main": "./out/extension",
    "repository": {
        "type": "git",
        "url": "https://github.com/christophschlosser/doxdocgen.git"
    },
    "bugs": {
        "url": "https://github.com/christophschlosser/doxdocgen/issues"
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.3",
        "vscode": "^1.1.5",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42",
        "tslint": "^5.7.0"
    }
}